<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel1</object-name>
    
 
    <!-- Objets transparents -->
  </animation>

  <!-- Gear Lever -->
  <animation>
	  <type>rotate</type>
	  <object-name>GearLever</object-name>
	  <property>controls/gear/gear-down</property>
	  <factor>50</factor>
	  <offset-deg>-50</offset-deg>   
	  <center>
	   <x-m>-0.0421</x-m>
	   <y-m>0</y-m>
	   <z-m>-0.0865</z-m>
	  </center>
	   <axis>  
	   <y>1</y>
	  </axis>
	</animation>
 
  <!-- Flaps lever -->
  <animation>
	  <type>rotate</type>
	  <object-name>panelC.027</object-name>
	  <property>controls/flight/flaps</property>
	  <factor>60</factor>
	  <offset-deg>-37</offset-deg>   
	  <axis>  
	   <x1-m>0.004</x1-m>
	   <y1-m>-0.1265</y1-m>
	   <z1-m>0.1000</z1-m>
	   <x2-m>0.004</x2-m>
	   <y2-m>-0.1145</y2-m>
	   <z2-m>0.1000</z2-m>
	  </axis>
	</animation>

  <!-- Park break lever -->
  <animation>
	  <type>rotate</type>
	  <object-name>PbrakeLever</object-name>
	  <property>controls/gear/brake-parking</property>
	  <factor>45</factor>
	  <offset-deg>0</offset-deg>   
	  <axis>  
	   <x1-m>0.1372</x1-m>
	   <y1-m>-0.1445</y1-m>
	   <z1-m>0.1498</z1-m>
	   <x2-m>0.1704</x2-m>
	   <y2-m>-0.1445</y2-m>
	   <z2-m>0.2453</z2-m>
	  </axis>
	</animation>
	<animation>
	  <type>translate</type>
	  <object-name>PbrakeLever</object-name>
	  <property>controls/gear/brake-parking</property>
	  <factor>0.03</factor>
	  <offset-m>-0.03</offset-m>
	  <axis>  
	   <x1-m>0.1372</x1-m>
	   <y1-m>-0.1445</y1-m>
	   <z1-m>0.1498</z1-m>
	   <x2-m>0.1704</x2-m>
	   <y2-m>-0.1445</y2-m>
	   <z2-m>0.2453</z2-m>
	  </axis>
	</animation>

  <!-- Speed brakes lever -->
